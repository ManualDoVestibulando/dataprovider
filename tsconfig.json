{
  "compilerOptions": {
    "target": "es2019",                  
    "module": "commonjs",              
    "outDir": "./lib/",                 
    "strict": true,                      
    "esModuleInterop": true,              
    "skipLibCheck": true,
    "noImplicitAny": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true
  }
}
